#: setup/main.py:85
#, python-format
msgid "Unknown format of engine name: IBUS_ENGINE_NAME=%(name)s"
msgstr "Unbekanntes Format des Enginenamens: IBUS_ENGINE_NAME=%(name)s"

#: setup/main.py:90
msgid "IBUS_ENGINE_NAME environment variable is not set."
msgstr "Die IBUS_ENGINE_NAME Umgebungsvariable variable is nicht gesetzt."

#: setup/main.py:94
msgid ""
"Cannot determine the config file for this engine. Please use the --config-"
"file option."
msgstr ""
"Die Konfigurationsdatei für diesen Engine kann nicht bestimmt werden. Bitte "
"benutzen Sie die --config-file Option."

#: setup/main.py:101
#, python-format
msgid "Config file %(file)s does not exist."
msgstr "Die Konfigurationsdatei %(file)s existiert nicht."

#: setup/main.py:113
msgid "ibus is not running."
msgstr "Ibus läuft nicht."

#: setup/main.py:119
#, python-format
msgid "Preferences for ibus-typing-booster \"%(symbol)s\""
msgstr "Ibus Typing Booster \"%(symbol)s\" Einstellungen"

